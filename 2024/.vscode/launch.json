{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build project",
            "program": "${workspaceFolder}/bin/Debug/net9.0/aoc.dll",
            "args": ["${fileBasenameNoExtension}"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}
